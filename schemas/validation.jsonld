{
  "@context": {
    "schema": "http://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "temp": "https://discovery.biothings.io/view/temp/"
  },
  "@graph": [
    {
      "@id": "temp:MyThing",
      "@type": "rdfs:Class",
      "rdfs:comment": "A temp type to try validation and cardinality",
      "rdfs:label": "MyThing",
      "rdfs:subClassOf": {
        "@id": "schema:Thing"
      },
      "$validation": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "properties": {
          "curator": {
            "description": "Curator of this thing.",
            "owl:cardinality": "many"
          },
          "url": {
            "description": "URL of the item.",
            "owl:cardinality": "many"
          },
          "sameAs": {
            "description": "URL of a reference Web page that unambiguously indicates the item's identity. E.g. the URL of the item's Wikipedia page, Wikidata entry, or official website.",
            "owl:cardinality": "many"
          },
          "identifier": {
            "description": "The identifier property represents any kind of identifier for any kind of [[Thing]], such as ISBNs, GTIN codes, UUIDs etc. Schema.org provides dedicated properties for representing many of these, either as textual strings or as URL (URI) links. See [background notes](/docs/datamodel.html#identifierBg) for more details.\n        ",
            "owl:cardinality": "one"
          }
        },
        "required": [
          "identifier"
        ],
        "recommended": [
          "url",
          "sameAs"
        ],
        "optional": [
          "curator"
        ]
      },
      "schemaVersion": [
        "https://schema.org/docs/releases.html#v23.0"
      ]
    },
    {
      "@id": "temp:curator",
      "@type": "rdf:Property",
      "rdfs:comment": "Curator of this thing.",
      "rdfs:label": "curator",
      "schema:domainIncludes": {
        "@id": "temp:MyThing"
      },
      "schema:rangeIncludes": [
        {
          "@id": "schema:Person"
        },
        {
          "@id": "schema:Organization"
        }
      ]
    }
  ]
}
